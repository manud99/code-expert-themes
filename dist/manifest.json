{
    "manifest_version": 3,
    "name": "Code Expert Themes",
    "description": "Add beautiful colors to Code Expert",
    "version": "0.1",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://expert.ethz.ch/ide2/*"
            ],
            "css": [
                "themes/nord_dark.css"
            ]
        }
    ],
    "action": {
        "default_icon": {
            "16": "icons/icon-16.png",
            "32": "icons/icon-32.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        },
        "default_popup": "popup.html"
    }
}